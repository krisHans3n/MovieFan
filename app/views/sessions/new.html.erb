<h1>Log in</h1>
<p id=”notice”><%= notice %></p>
<%= form_for(:session, url: login_path) do |f| %>

<%= f.label :email %>
<%= f.email_field :email%>

<%= f.label :password %>
<%= f.password_field:password %>

<%= f.submit 'Login',class: 'btn btn-primary' %>
<% end %>
<p>Are you new? <%= link_to 'Sign up',new_user_path %></p>