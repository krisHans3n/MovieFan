<p id="notice"><%= notice %></p>

<h1>Movieswatcheds</h1>

<table>
  <thead>
    <tr>
      <th>Movies</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @movieswatcheds.each do |movieswatched| %>
      <tr>
        <td><%= movieswatched.movies %></td>
        <td><%= link_to 'Show', movieswatched %></td>
        <td><%= link_to 'Edit', edit_movieswatched_path(movieswatched) %></td>
        <td><%= link_to 'Destroy', movieswatched, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Movieswatched', new_movieswatched_path %>
